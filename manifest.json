{
  "manifest_version": 2,

  "name": "DataSection Mobile Search",
  "description": "This extension will analyze help you quickly find people on facebook via phone number",
  "version": "1.3.0",
  "background": {
      "scripts": [ "script.js" ]
   },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.1.0.min.js", "contentscript.js"]
    }
  ],
  "icons": {
      "128": "128x128.png",
      "16": "16x16.png",
      "32": "32x32.png"
   },
  "permissions": [ "tabs", "contextMenus", "activeTab", "cookies", "http://*/*", "https://*/*" ],
  "commands": {
		"find" : {
			"suggested_key": {
				 "default": "Alt+S" 
			},
			"description": "Find user"
		}
	}
}